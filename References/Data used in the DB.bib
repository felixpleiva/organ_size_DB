	note = {Place: 6-9 CARLTON HOUSE TERRACE, LONDON SW1Y 5AG, ENGLAND
Publisher: ROYAL SOC
Section: 0},
	keywords = {aquaculture, Atlantic salmon, allometry, liver, sexual dimorphism, heart morphology},
	annote = {RAYYAN-INCLUSION: "Felix"={\textgreater}"Included"},
