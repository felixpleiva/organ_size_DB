Publisher: COMPANY OF BIOLOGISTS LTD
Section: 0},
	keywords = {body mass, food intake, morphology, mouse, Mus musculus, resting metabolic rate, selection},
	pages = {777--784},
	annote = {RAYYAN-INCLUSION: "Felix"={\textgreater}"Included"},
}

