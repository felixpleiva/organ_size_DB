@article{damaziak_effect_2014,
	title = {Effect of genotype on selected quality attributes of {Turkey} bone; [{Einfluss} des {Genotyps} auf ausgewählte {Qualitätsmerkmale} von {Putenknochen}]},
	volume = {78},
	issn = {16129199},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84929593962&doi=10.1399%2feps.2014.12&partnerID=40&md5=6c861483c50d81c791c4d5313e3a4c98},
	doi = {10.1399/eps.2014.12},
	abstract = {The experiment was carried out with male turkeys belonging to two genotypes. The SG group consisted of slowgrowing hybrids of local breeds of turkeys reared in Poland. Their growth rate, body musculature and plumage color reflected the American conservative breeds belonging to Heritage Turkeys. The FG group included commercial, heavy BUT Big 6 hybrids. All poults were hatched under the same incubation conditions. Each group comprised 80 birds. The turkeys were reared until the 22nd week of life and had access to a free range area. The aim of this study has been to determine the differences in the quality of femur (osteometric and strength features) originating from slow-growing (SG) and fast-growing (FG) turkeys. In the future this will allow for developing hybrid turkeys intended for organic and free-range production. It has been assumed that if there are differences in bone strength, then they can be detected by means of instrumental research methods: mechanical and contact acoustic emission. In addition, the relationships between the mechanical, acoustic and osteometric parameters have been studied. The osteometric properties of the femur differed distinctly between the fast-growing (FG) and slow-growing (SG) turkeys. The breaking resistance of femoral bones depended on turkey genotype. The bones of heavier FG birds were stronger than those of SG turkeys, measured by breaking and acoustic energy tests. The osteometric characteristics (density, thickness and the length of the shaft, weight) of turkey bones strongly negatively correlated with breaking stress and positively with the strength, fracture energy and the number of acoustic emission events. However, the proportion of the femur bone weight related to body weight strongly positively correlated with bone breaking stress. © Verlag Eugen Ulmer, Stuttgart.},
	language = {English},
	journal = {European Poultry Science},
	author = {Damaziak, K. and Michalczuk, Monika and Szara, T. and Marzec, A. and Niemiec, J. and Turek, B.},
	year = {2014},
	note = {Publisher: Verlag Eugen Ulmer
Section: 0},
