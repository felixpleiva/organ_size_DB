	note = {Place: BIDDER BUILDING CAMBRIDGE COMMERCIAL PARK COWLEY RD, CAMBRIDGE, CAMBS, ENGLAND CB4 4DL
Publisher: COMPANY OF BIOLOGISTS LTD
Section: 0},
	keywords = {kidney, mouse, nitrogen filtration rate, organ mass, protein intake},
	pages = {2081--2090},
