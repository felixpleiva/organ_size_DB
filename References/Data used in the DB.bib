	year = {2013},
	note = {Place: 111 RIVER ST, HOBOKEN 07030-5774, NJ USA
Publisher: WILEY
Section: 0},
	keywords = {immune response, chick, cytokine, beak trimming stress, gamma-aminobutyric acid},
