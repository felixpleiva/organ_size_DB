	journal = {SCIENTIFIC REPORTS},
	author = {Wegner, Marcin and Kokoszynski, Dariusz and Zochowska-Kujawska, Joanna and Kotowicz, Marek and Wlodarczyk, Karol and Banaszewska, Dorota and Batkowska, Justyna},
