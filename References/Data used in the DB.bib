	title = {The index of the hearts squad {Carnivora}, as an indicator of predator ecology - {ECOLOGICAL} {AND} {BIOLOGICAL} {WELL}-{BEING} {OF} {FLORA} {AND} {FAUNA} ({EBWFF}-2020)},
