Publisher: WILEY-BLACKWELL
Section: 0},
	keywords = {diet, brain size, platyrrhines, Expensive Tissue Hypothesis, gut size, New World monkeys, sociality},
	pages = {2207--2221},
	annote = {RAYYAN-INCLUSION: "Felix"={\textgreater}"Included"},
